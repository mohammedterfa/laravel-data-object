{
    "name": "mohammedterfa/laravel-data-object",
    "description": "A set of tools to make working with Data Transfer Objects easier in Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "role": "Developer",
            "name": "Mohammed Terfa",
            "email": "mohammedterfa@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mohammedterfa\\DataObjects\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mohammedterfa\\DataObjects\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.47",
        "illuminate/support": "^10.47"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
